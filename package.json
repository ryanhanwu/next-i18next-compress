{
  "name": "@devoxa/next-i18next-compress",
  "description": "Automatically compress locale keys for `next-i18next`",
  "version": "1.3.2",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/devoxa/next-i18next-compress"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --ignore-path='.prettierignore' --list-different --write .",
    "format:check": "prettier --ignore-path='.prettierignore' --check .",
    "lint": "eslint '{src,tests}/**/*.ts'",
    "build": "rm -rf dist/ && tsc",
    "preversion": "yarn build"
  },
  "prettier": "@devoxa/prettier-config",
  "main": "dist/src/babel.js",
  "exports": {
    ".": "./dist/src/babel.js",
    "./babel": "./dist/src/babel.js",
    "./swc": "./dist/src/swc.js",
    "./config": "./config/node.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "config/",
    "babel.js"
  ],
  "dependencies": {
    "@swc/core": "^1.13.5",
    "colors": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@devoxa/eslint-config": "4.0.2",
    "@devoxa/prettier-config": "2.0.3",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "20.9.0"
  }
}
